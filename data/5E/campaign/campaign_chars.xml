<root>
	<windowclass name="charselect_host" merge="join">
		<sheetdata>
			<button_charwizard name="button_characterwizard" insertbefore="filter">
				<anchored to="rightanchor">
					<top />
					<right anchor="left" relation="relative" offset="-5" />
				</anchored>
			</button_charwizard>
		</sheetdata>
	</windowclass>

	<windowclass name="charselect_client" merge="join">
		<sheetdata>
			<button_charwizard name="button_characterwizard" insertbefore="filter">
				<anchored to="rightanchor">
					<top />
					<right anchor="left" relation="relative" offset="-5" />
				</anchored>
			</button_charwizard>
		</sheetdata>
	</windowclass>

	<windowclass name="charselect_local" merge="join">
		<sheetdata>
			<button_charwizard name="button_characterwizard" insertbefore="filter">
				<anchored to="rightanchor">
					<top />
					<right anchor="left" relation="relative" offset="-5" />
				</anchored>
			</button_charwizard>
		</sheetdata>
	</windowclass>
</root>
